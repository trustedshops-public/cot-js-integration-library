{
    "name": "@trustedshops-public/cot-integration-library",
    "version": "1.11.0",
    "private": false,
    "type": "module",
    "main": "dist/index.umd.js",
    "module": "dist/index.es.js",
    "types": "dist/index.d.ts",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/trustedshops-public/cot-js-integration-library.git"
    },
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.es.js",
            "require": "./dist/index.umd.js"
        }
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "dev": "tsx watch --env-file=.env.local ./devserver/server.tsx",
        "build": "tsc && vite build --config vite.library.config.ts",
        "test": "npx vitest --silent",
        "prepare": "husky",
        "generate-ssl-cert": "cd certs && mkcert -install && mkcert localhost"
    },
    "dependencies": {
        "events": "^3.3.0",
        "jose": "^6.0.11",
        "node-cache": "^5.1.2",
        "winston": "^3.17.0"
    },
    "devDependencies": {
        "@commitlint/cli": "^19.8.1",
        "@commitlint/config-conventional": "^19.8.1",
        "@types/cookie-parser": "^1.4.9",
        "@types/events": "^3.0.3",
        "@types/express": "^5.0.0",
        "@types/node": "22.18.6",
        "@types/react": "^19.1.8",
        "@types/react-dom": "^19.1.6",
        "@vitejs/plugin-react": "^5.0.0",
        "commitlint": "^19.8.1",
        "cookie-parser": "^1.4.7",
        "dotenv": "^17.0.0",
        "express": "^5.0.0",
        "husky": "^9.1.7",
        "react": "^19.1.0",
        "react-dom": "^19.1.0",
        "tsx": "^4.20.3",
        "typescript": "^5.8.3",
        "vite": "^7.0.0",
        "vite-plugin-dts": "^4.5.4",
        "vite-plugin-node-polyfills": "^0.24.0",
        "vite-plugin-static-copy": "^3.0.2",
        "vitest": "^3.2.4"
    },
    "publishConfig": {
        "access": "public"
    }
}
