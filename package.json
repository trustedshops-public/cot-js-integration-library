{
    "name": "@trustedshops-public/cot-integration-library",
    "version": "0.0.0",
    "type": "module",
    "main": "dist/index.umd.js",
    "module": "dist/index.es.js",
    "types": "dist/index.d.ts",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/trustedshops-public/cot-js-integration-library.git"
    },
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.es.js",
            "require": "./dist/index.umd.js"
        }
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "dev": "vite --config vite.config.ts",
        "build": "tsc && vite build --config vite.config.ts ",
        "test": "jest",
        "preview": "vite preview",
        "prepare": "husky",
        "generate-ssl-cert": "cd certs && mkcert -install && mkcert localhost"
    },
    "dependencies": {
        "jose": "^5.9.6",
        "jsonwebtoken": "^9.0.2",
        "jwk-to-pem": "^2.0.7",
        "node-cache": "^5.1.2",
        "winston": "^3.17.0"
    },
    "devDependencies": {
        "@commitlint/cli": "19.6.1",
        "@commitlint/config-conventional": "19.6.0",
        "@types/jest": "^29.5.14",
        "@types/jsonwebtoken": "^9.0.7",
        "@types/jwk-to-pem": "^2.0.3",
        "@types/node": "22.10.5",
        "commitlint": "19.6.1",
        "husky": "9.1.7",
        "jest": "^29.7.0",
        "ts-jest": "^29.2.5",
        "ts-node": "^10.9.2",
        "typescript": "^5.4.2",
        "vite": "6.0.7",
        "vite-plugin-dts": "4.4.0",
        "vite-plugin-static-copy": "2.2.0"
    }
}